<table>
  <tr>
    <th>Name</th>
    <th>Date</th>
  </tr>

<% @trips.each do |trip| %>
  <tr>
    <td><%= link_to trip.name, trip %></td>
    <td><%=h trip.date %></td>
    <td><%= render :partial => "/shared/crud_links_edit_delete_small", :locals => {
        :obj => trip
     } %></td>
  </tr>
<% end %>
</table>

