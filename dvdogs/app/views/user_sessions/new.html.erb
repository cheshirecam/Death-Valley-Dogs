   <h1>Log in</h1>  

     
   <% form_for @user_session do |f| %>  
   <%= f.error_messages %>  

  <p>
    <%= f.label :login %><br />
    <%= f.text_field :login %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
       <%= f.submit "Submit" %>  
   <% end %>  